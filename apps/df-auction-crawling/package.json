{
  "name": "df-auction-crawling",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "@neoavath/neople-api": "workspace:^",
    "@nestjs/common": "^10.4.17",
    "@nestjs/core": "^10.4.17",
    "typescript": "~5.7.2",
    "@nestjs/testing": "^10.4.17"
  },
  "scripts": {
    "start": "nest start"
  },
  "nx": {
    "name": "df-auction-crawling",
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "webpack-cli build",
          "args": [
            "node-env=production"
          ]
        },
        "configurations": {
          "development": {
            "args": [
              "node-env=development"
            ]
          }
        }
      },
      "serve": {
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "df-auction-crawling:build",
          "runBuildTargetDependencies": false
        },
        "configurations": {
          "development": {
            "buildTarget": "df-auction-crawling:build:development"
          },
          "production": {
            "buildTarget": "df-auction-crawling:build:production"
          }
        }
      },
      "test": {
        "options": {
          "passWithNoTests": true
        }
      }
    }
  }
}
